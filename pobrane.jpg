<?php

$radius = $_POST['radius']
$file = $_POST['file']

$options = [
    'http' => [
        'header' => "Content-type: application/json",
        'method' => "POST",
        'content' => http_build_query([
            "radius" => $radius,
            "file" => $file
        ])
    ]
]

$context = stream_context_create($options)
$result = file_get_contents("localhost:8080/upload", false, $context);
